<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Proyecto.Views"
    x:Class="Proyecto.AppShell">

    <FlyoutItem Title="Iniciar Sesion" Icon="login.png">
        <ShellContent Route="LoginPage" ContentTemplate="{DataTemplate views:LoginPage}" />
    </FlyoutItem>

    <FlyoutItem Title="Usuarios" Icon="users.png">
        <ShellContent Route="UsuariosPage" ContentTemplate="{DataTemplate views:UsuariosPage}" />
    </FlyoutItem>

</Shell>
